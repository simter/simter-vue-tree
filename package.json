{
  "name": "simter-vue-tree",
  "version": "0.1.0",
  "description": "A vue tree component",
  "keywords": [
    "simter",
    "vue",
    "tree",
    "parcel"
  ],
  "main": "dist/simter-vue-tree.js",
  "module": "dist/simter-vue-tree.esm.js",
  "repository": "https://github.com/simter/simter-vue-tree.git",
  "author": "RJ.Hwang <rongjihuang@gmail.com>",
  "license": "MIT",
  "files": [
    "src",
    "dist",
    "docs"
  ],
  "scripts": {
    "start": "parcel demo/index.html -d temp",
    "test": "ava",
    "clean": "rm -rf dist/* temp/*",
    "build": "yarn run clean && yarn test && parcel build demo/index.html -d dist",
    "prepublishOnly": "yarn run build"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^2.3.0",
    "ava": "~0.25.0",
    "babel-core": "~6.26.3",
    "babel-preset-env": "~1.7.0",
    "browser-env": "~3.2.5",
    "parcel-bundler": "~1.10.3",
    "require-extension-hooks": "~0.3.2",
    "require-extension-hooks-babel": "~0.1.1",
    "require-extension-hooks-vue": "~1.1.0",
    "stylus": "~0.54.5",
    "vue": "~2.5.17",
    "vue-hot-reload-api": "^2.3.1",
    "vue-template-compiler": "~2.5.17"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "./test/helpers/setup.js"
    ]
  }
}